{
  "short_name": "ReCxt",
  "name": "React Chrome Extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "favicon.ico",
    "48": "logo192.png",
    "128": "logo512.png"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "options_page": "index.html"
}
